{
  "name": "react-styleguidist",
  "description": "React components styleguide",
  "version": "0.1.1",
  "homepage": "https://github.com/sapegin/react-styleguidist",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sapegin/react-styleguidist.git"
  },
  "bugs": {
    "url": "https://github.com/sapegin/react-styleguidist/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/sapegin/react-styleguidist/blob/master/License.md"
    }
  ],
  "main": "src/build.js",
  "bin": {
    "styleguidist": "bin/styleguidist"
  },
  "files": [
    "bin",
    "src",
    "loaders"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "babel-plugin-react-transform": "1.0.5",
    "chalk": "1.1.1",
    "css-loader": "0.17.0",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "0.8.2",
    "findup": "0.1.5",
    "glob": "5.0.14",
    "html-webpack-plugin": "1.6.1",
    "lodash": "3.10.1",
    "marked": "0.3.5",
    "minimist": "1.2.0",
    "postcss-advanced-variables": "1.0.0",
    "postcss-loader": "0.6.0",
    "prettyjson": "1.1.3",
    "react-codemirror": "0.1.5",
    "react-docgen": "2.1.2",
    "react-tools": "0.13.3",
    "react-transform-catch-errors": "0.1.3",
    "react-transform-webpack-hmr": "0.1.6",
    "redbox-react": "1.0.6",
    "sanitize.css": "2.0.0",
    "style-loader": "0.12.4",
    "webpack": "1.12.2",
    "webpack-dev-middleware": "1.2.0",
    "webpack-dev-server": "1.10.1",
    "webpack-hot-middleware": "2.3.0",
    "webpack-merge": "0.2.0"
  },
  "peerDependencies": {
    "react": ">=0.13.0"
  },
  "devDependencies": {
    "babel-eslint": "4.1.3",
    "eslint": "1.5.1",
    "eslint-plugin-react": "3.4.2",
    "gulp": "3.9.0",
    "gulp-gh-pages": "0.5.2"
  },
  "scripts": {
    "test": "npm run lint && npm run build",
    "start": "./bin/styleguidist server --config example/styleguide.config.js",
    "lint": "eslint src --ext .js",
    "build": "./bin/styleguidist build --config example/styleguide.config.js",
    "publish": "npm run build && gulp gh-pages"
  },
  "keywords": [
    "react",
    "styleguide",
    "style guide",
    "documentation",
    "docs",
    "generator",
    "components"
  ]
}
